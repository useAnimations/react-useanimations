{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "arrow-parens": "off",
    "react/prop-types": "off",
    "react/jsx-props-no-spreading": "off",
    "rest-spread-spacing": "off",
    "react/state-in-constructor": "off",
    "object-curly-newline": "off"
  }
}
